{
    "name": "eliseekn/laravel-metrics",
    "description": "Generate easily metrics and trends data of your models for your dashboards.",
    "keywords": [
        "laravel-package",
        "charts",
        "laravel",
        "dashboard"
    ],
    "homepage": "https://github.com/eliseekn/laravel-metrics",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "N'Guessan Kouadio Elis√©e",
            "email": "eliseekn@gmail.com",
            "homepage": "https://eliseekn.netlify.app",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "illuminate/database": "^10.20",
        "illuminate/support": "^10.20",
        "nesbot/carbon": "^2.69"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "Eliseekn\\LaravelMetrics\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eliseekn\\LaravelMetrics\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "aliases": {
                "LaravelMetrics": "Eliseekn\\LaravelMetrics\\LaravelMetricsFacade"
            }
        }
    }
}
